{
  "name": "chesswebsite",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node bin/www"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "google"
    ],
    "rules": {
      "require-jsdoc": "off",
      "indent": [
        2,
        4
      ],
      "new-cap": "off"
    },
    "env": {
      "es6": true,
      "node": true
    },
    "parser": "babel-eslint"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "espree": "^7.3.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "helmet": "^4.1.0",
    "http-errors": "^1.8.0",
    "mongoose": "^5.10.4",
    "morgan": "^1.10.0",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^6.0.1",
    "pug": "^3.0.0",
    "socket.io": "^2.3.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^7.8.1",
    "eslint-config-google": "^0.14.0"
  }
}
