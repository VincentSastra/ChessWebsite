{
  "name": "chess-client",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "eject": "react-scripts eject",
    "serve": "serve -s build -l 80"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "eslint:recommended",
      "google"
    ],
    "rules": {
      "require-jsdoc": "off",
      "indent": [
        "error",
        2
      ],
      "linebreak-style": ["error", "windows"],
      "new-cap": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@craco/craco": "^5.7.0",
    "@testing-library/jest-dom": "^5.11.5",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "antd": "^4.7.3",
    "craco-antd": "^1.19.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.0",
    "socket.io-client": "^2.3.1"
  },
  "devDependencies": {
    "gh-pages": "^3.1.0"
  }
}
